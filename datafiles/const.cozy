import cozy.std;

/// const variable
const TEXT = "Hello, World!";

print(TEXT);

/// const func

const func Hey() {
	print("Hello, World!");
}

Hey();

/// const class

const class Class() {
	property value = undefined;
	
	constructor {
		this.value = "Hello, World!";
	}
}

print(new Class().value);

